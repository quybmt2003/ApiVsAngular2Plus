{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config ClientApp/config/webpack.prod.js --progress --profile --bail",
    "build:dev": "rimraf dist && webpack --config ClientApp/config/webpack.dev.js --progress --profile",
    "webpack-dev": "cross-env NODE_ENV=development webpack",
    "webpack-prod": "cross-env NODE_ENV=production webpack"
  },
  "license": "MIT",
